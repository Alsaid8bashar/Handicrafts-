<section class="signin-page account">
  <div class="container">
    <div class="row">
      <div class="col-md-6 col-md-offset-3">
        <div class="block text-center">
          <a class="logo" href="~/home/index.html">
            <img src="~/home/images/logo.png" alt="">
          </a>
          <h2 class="text-center">Welcome Back</h2>
          <form class="text-left clearfix" method="POST" asp-action="Login">
            <div class="form-group">
              <input type="text" class="form-control" asp-for="UserrName"  placeholder="User name" required>
            </div>

            <div class="form-group">
              <input type="password" class="form-control"asp-for="Passwordd"  placeholder="Password" required>
            </div>
            <div class="text-center">
              <button type="submit" class="btn btn-main text-center" >Login</button>
            </div>
          </form>
          <p class="mt-20">New in this site ?<a asp-action="Register"> Create New Account</a></p>
        </div>
      </div>
    </div>
  </div>
</section>

