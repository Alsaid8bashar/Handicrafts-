<body id="body">

<!-- Start Top Header Bar -->
<section class="top-header">
	<div class="container">
		<div class="row">
			<div class="col-md-4 col-xs-12 col-sm-4">
				<div class="contact-number">
					<i class="tf-ion-ios-telephone"></i>
					<span>0129- 12323-123123</span>
				</div>
			</div>
			<div class="col-md-4 col-xs-12 col-sm-4">
				<!-- Site Logo -->
				<div class="logo text-center">
					<a asp-action="Index">
						<!-- replace logo here -->
						<svg width="135px" height="29px" viewBox="0 0 155 29" version="1.1" xmlns="http://www.w3.org/2000/svg"
							xmlns:xlink="http://www.w3.org/1999/xlink">
							<g id="Page-1" stroke="none" stroke-width="1" fill="none" fill-rule="evenodd" font-size="40"
								font-family="AustinBold, Austin" font-weight="bold">
								<g id="Group" transform="translate(-108.000000, -297.000000)" fill="#000000">
									<text id="AVIATO">
										<tspan x="108.94" y="325">AVIATO</tspan>
									</text>
								</g>
							</g>
						</svg>
					</a>
				</div>
			</div>
			<div class="col-md-4 col-xs-12 col-sm-4">
				<!-- Cart -->
				<ul class="top-menu text-right list-inline">
					<li class="dropdown cart-nav dropdown-slide">
						<a href="~/home/#!" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown"><i
								class="tf-ion-android-cart"></i>Cart</a>
						<div class="dropdown-menu cart-dropdown">
								@{

									List<CartProduct> carts = ViewBag.cart;
									int Sum = 0;
								}
							<!-- Cart Item -->
								@foreach (var item in carts)
								{	<h1>@ViewBag.namme</h1>
									if (item.Cart.Purchasestate == 0)
									{
										<div class="media">
											<a class="pull-left" href="~/home/#!">
												<img class="media-object" src="@Url.Content("~/Images/" + item.Product.Imagepath)" alt="image" />
											</a>
											<div class="media-body">
												<h4 class="media-heading"><a href="~/home/#!">@item.Product.Name</a></h4>
												<div class="cart-price">
													<span>@item.Quantity x</span>
													<span>@item.Product.Price</span>
												</div>
												<h5><strong>$@item.Product.Price</strong></h5>
											</div>
											<a asp-action="RemoveFromCart" asp-controller="User" asp-route-id="@item.Id2" class="remove"><i class="tf-ion-close"></i></a>
										</div>

										<!-- / Cart Item -->
										Sum += (int)item.Product.Price*(int)item.Quantity;
										<!-- Cart Item -->
									}
								}



								<div class="cart-summary">
								<span>Total</span>
								<span class="total-price">$ @Sum </span>
							</div>
							<ul class="text-center cart-buttons">
								<li><a asp-action="Cart" class="btn btn-small">View Cart</a></li>
								<li><a asp-action="Checkout" class="btn btn-small btn-solid-border">Checkout</a></li>
							</ul>
						</div>

					</li><!-- / Cart -->

					<!-- Search -->
					<li class="dropdown search dropdown-slide">
						<a class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown"><i
								class="tf-ion-ios-search-strong"></i> Search</a>
						<ul class="dropdown-menu search-dropdown">
							<li>
								<form  asp-action="Search" ><input type="search" name="catgeoryName" class="form-control" placeholder="Search..."></form>
							</li>
						</ul>
					</li><!-- / Search -->

					<!-- Languages -->
					<li class="commonSelect">
						<select class="form-control">
							<option>EN</option>
							<option>DE</option>
							<option>FR</option>
							<option>ES</option>
						</select>
					</li><!-- / Languages -->

				</ul><!-- / .nav .navbar-nav .navbar-right -->
			</div>
		</div>
	</div>
</section><!-- End Top Header Bar -->


