										<li role="separator" class="divider"></li>
										<li><a asp-action="Login" asp-controller="LoginandRegistration">Log out</a></li>

										
										<li><a href="~/home/forget-password.html">Forget Password</a></li>
									</ul>
								</div>

								<!-- Mega Menu -->
								<div class="col-sm-3 col-xs-12">
									<a href="~/home/shop.html">
										<img class="img-responsive" src="~/home/images/shop/header-img.jpg" alt="menu image" />
									</a>
								</div>
							</div><!-- / .row -->
						</div><!-- / .dropdown-menu -->
					</li><!-- / Pages -->



					<!-- Blog -->
			

			
				</ul><!-- / .nav .navbar-nav -->

			</div>
			<!--/.navbar-collapse -->
		</div><!-- / .container -->
	</nav>
</section>












